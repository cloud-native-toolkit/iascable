{
  "name": "@cloudnativetoolkit/iascable",
  "version": "3.0.0-beta.11",
  "description": "",
  "keywords": [],
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "types": "./build/src/index.d.ts",
  "files": [
    "dist",
    "ref-arch",
    "scripts"
  ],
  "author": "Sean Sundberg <seansund@us.ibm.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloud-native-toolkit/iascable.git"
  },
  "license": "MIT",
  "bin": {
    "iascable": "build/cjs/script.js"
  },
  "pkg": {
    "scripts": "build/cjs/script.js",
    "assets": [
      "build/**/*"
    ],
    "targets": [
      "node16-linux-x64",
      "node16-macos-x64",
      "node16-alpine-x64",
      "node16-linux-arm64",
      "node16-macos-arm64",
      "node16-alpine-arm64"
    ]
  },
  "engines": {
    "node": "^18.7.0 || >= 16",
    "npm": "^8.16.0 || >= 8"
  },
  "volta": {
    "node": "18.7.0",
    "npm": "8.16.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "watch": {
    "build": "src/*.ts"
  },
  "scripts": {
    "setup": "node -r ./esbuild-hook ./scripts/setup",
    "build": "npm run clean && npm run type:dts && npm run build:main",
    "build:main": "node -r ./esbuild-hook ./scripts/build",
    "postbuild": "npm run postbuild:cjs && npm run postbuild:esm",
    "postbuild:cjs": "copyfiles -u 1 src/**/*.liquid build/cjs/ && chmod +x build/cjs/script*.js",
    "postbuild:esm": "copyfiles -u 1 src/**/*.liquid build/esm/ && chmod +x build/esm/script*.js",
    "clean": "node -r ./esbuild-hook ./scripts/clean",
    "type:dts": "tsc --emitDeclarationOnly",
    "type:check": "tsc --noEmit",
    "format": "prettier \"src/**/*.ts\" --write",
    "format:check": "prettier \"src/**/*.ts\" --check",
    "lint": "eslint src --ext .ts --fix",
    "lint:check": "eslint src --ext .ts",
    "test": "jest",
    "tdd": "jest --coverage --watch",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "spell:check": "cspell \"{README.md,CODE_OF_CONDUCT.md,CONTRIBUTING.md,.github/*.md,src/**/*.ts}\"",
    "cz": "cz",
    "semantic-release": "semantic-release",
    "pkg": "pkg --out-path pkg-bin --options no-warnings .",
    "watch": "npm-watch",
    "iascable": "ts-node src/script.ts"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "devDependencies": {
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/github": "^8.0.6",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/event-stream": "^4.0.0",
    "@types/jest": "^29.2.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.camelcase": "^4.3.7",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/lodash.unionby": "^4.8.7",
    "@types/lodash.uniq": "^4.5.7",
    "@types/lodash.uniqby": "^4.7.7",
    "@types/lodash.uniqwith": "^4.5.7",
    "@types/node": "^16.18.2",
    "@types/prompts": "^2.4.1",
    "@types/superagent": "^4.1.15",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "c8": "^7.12.0",
    "colors": "^1.3.2",
    "commitizen": "^4.2.4",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.1",
    "cspell": "^6.12.0",
    "esbuild": "^0.15.12",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.2.2",
    "npm-watch": "^0.11.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "prompts": "^2.4.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.5",
    "shelljs": "^0.8.3",
    "slugify": "^1.6.5",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^9.0.0",
    "typedoc": "^0.23.18",
    "typescript": "^4.8.4",
    "vitest": "^0.24.3"
  },
  "dependencies": {
    "compare-versions": "^5.0.1",
    "event-stream": "^4.0.1",
    "js-yaml": "^4.0.0",
    "jszip": "^3.10.1",
    "liquidjs": "^9.42.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.unionby": "^4.8.0",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "lodash.uniqwith": "^4.5.0",
    "rxjs": "^7.5.7",
    "simple-git": "^3.14.1",
    "superagent": "^8.0.3",
    "typescript-ioc": "^3.2.2",
    "yargs": "^17.2.1",
    "z-schema": "^5.0.4"
  },
  "homepage": "https://github.com/cloud-native-toolkit/iascable.git#readme",
  "overrides": {
    "semver-regex": "3.1.4"
  }
}
